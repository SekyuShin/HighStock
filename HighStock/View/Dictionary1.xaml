<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:HighStock">
    <DataTemplate x:Key="DataTemplate_index">
        <TextBlock Text="{Binding Index}"></TextBlock>
    </DataTemplate>
    <DataTemplate x:Key="DataTemplate_name">
        <TextBlock Text="{Binding StockName}"></TextBlock>
    </DataTemplate>
    <DataTemplate x:Key="DataTemplate_close">
        <TextBlock Text="{Binding ShowClose}"></TextBlock>
    </DataTemplate>
    <DataTemplate x:Key="DataTemplate_changes">
        <TextBlock Text="{Binding ShowChanges}"></TextBlock>
    </DataTemplate>
    <DataTemplate x:Key="DataTemplate_changeratio">
        <TextBlock Text="{Binding StockChagesRatio}"></TextBlock>
    </DataTemplate>
    <DataTemplate x:Key="DataTemplate_volume">
        <TextBlock Text="{Binding ShowVolume}"></TextBlock>
    </DataTemplate>
</ResourceDictionary>
