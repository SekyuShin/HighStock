<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:HighStock">
    <DataTemplate x:Key="DataTemplate_bindex">
        <TextBlock Text="{Binding Index}"></TextBlock>
    </DataTemplate>
    <DataTemplate x:Key="DataTemplate_bname">
        <TextBlock Text="{Binding StockName}"></TextBlock>
    </DataTemplate>
    <DataTemplate x:Key="DataTemplate_bullish">
        <TextBlock Text="{Binding StockChagesRatio}"></TextBlock>
    </DataTemplate>
</ResourceDictionary>
